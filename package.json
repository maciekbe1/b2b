  "private": true,
  "dependencies": {
    "autoprefixer": "^10.2.5",
    "date-fns": "^2.21.3",
    "emailjs-com": "^2.6.4",
    "html-react-parser": "^1.2.6",
    "postcss": "^8.3.0",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.2",
    "react": "^17.0.2",
    "react-cookie-consent": "^6.2.4",
    "react-datepicker": "^3.8.0",
    "react-dom": "^17.0.2",
    "react-ga": "^3.3.0",
