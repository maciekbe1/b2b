    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.8",
    "autoprefixer": "^10.2.5",
    "axios": "^0.21.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "date-fns": "^2.21.2",
    "dotenv": "^9.0.0",
    "express": "^4.17.1",
    "html-react-parser": "^1.2.6",
    "http-proxy-middleware": "^1.3.1",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.0",
    "nodemailer": "^6.6.0",
    "nodemon": "^2.0.7",
    "postcss": "^8.2.14",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.1",
    "react": "^17.0.2",
    "react-cookie-consent": "^6.2.3",
    "react-datepicker": "^3.8.0",
    "react-dom": "^17.0.2",
    "react-ga": "^3.3.0",
    "react-helmet": "^6.1.0",
    "react-image-lightbox": "^5.1.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-select": "^4.3.0",
    "tailwindcss": "^2.1.2",
    "web-vitals": "^1.1.2",
    "winston": "^3.3.3"
  },
  "scripts": {
    "postbuild": "react-snap",
    "start": "nodemon --exec babel-node app.js",
    "react": "npm run watch:css && react-scripts start",
    "build": "npm run build:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:css": "postcss ./src/styles/tailwind.css -o ./src/styles/main.css",
    "watch:css": "postcss ./src/styles/tailwind.css -o ./src/styles/main.css",
    "prod": "node ./app.js",
    "log": "node restart.js > ./logs/app.log 2>&1",
    "dev": "concurrently \"npm run start\" \"npm run app\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.14.1",
    "@tailwindcss/custom-forms": "^0.2.1",
    "concurrently": "^6.0.2",
    "react-snap": "^1.23.0"
  }
}
